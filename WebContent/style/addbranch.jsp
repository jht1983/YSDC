<%@ page contentType="text/html;charset=GBK"%><link href='../css/table.css' rel='stylesheet' type='text/css'/><%	String strBranchId=request.getParameter("code");%><script type='text/javascript' src='../js/evenfunction.js'></script><body style="margin:0;padding:0;border:0;"><table align="center" width="100%" height="100%" cellpadding='0' cellspacing='0' border="0">	<tr>		<td height="20">			<table align="left" class='bttoparea'>				<tr>					<td height="20">					<%=com.yulongtao.pub.Pub.getBttn(com.yulongtao.pub.Pub.getBttnText("add.png","新建"), "miniWin('新建部门','','View?SPAGECODE=1372920360953&PCODE="+strBranchId+"',500,320,'','');","button green")%>					<td>				<%=com.yulongtao.pub.Pub.getBttn(com.yulongtao.pub.Pub.getBttnText("edit.png","修改"), "miniWin('修改部门','','View?SPAGECODE=1412048084955&scode="+strBranchId+"',500,320,'','');","button blue")%>				<%=com.yulongtao.pub.Pub.getBttn(com.yulongtao.pub.Pub.getBttnText("del.png","删除"), "delBranchId('"+strBranchId+"');","button red")%>				<%=com.yulongtao.pub.Pub.getBttn(com.yulongtao.pub.Pub.getBttnText("add.png","放大"), "branchgraph.yltTreeGraph.zoomGraph(true);","button red")%>				<%=com.yulongtao.pub.Pub.getBttn(com.yulongtao.pub.Pub.getBttnText("sub.png","缩小"), "branchgraph.yltTreeGraph.zoomGraph(false);","button gray")%>				</tr>			</table>		</td>	</tr>	<tr>		<td><iframe name='branchgraph' src="../comp?sys_type=branch&paramid=001&sys_root=<%=strBranchId%>" width="100%" height="100%" frameborder="0"></iframe></td>	</tr></table></body><script type='text/javascript' src='../js/ylminiutil.js'></script><script>	function delBranchId(_strBranchId){				var sys_objParentLeftFrame=parent.lxleft;				var vResult=getTx("comid=008&sbrid="+_strBranchId,"../docommand");		if(vResult=="true")			sys_objParentLeftFrame.yltTree.prototype.fushTree("testtree0");	}</script>